# Backend Configuration
BACKEND_URLS=["http://localhost:8001", "http://localhost:8002"]

# Load Balancing
LOAD_BALANCE_STRATEGY="least_connections"

# Health Check Settings
HEALTH_CHECK_INTERVAL=10
HEALTH_CHECK_TIMEOUT=5
HEALTH_CHECK_RETRIES=3

# Connection Pool Settings
MAX_CONNECTIONS=100
CONNECTION_TIMEOUT=30
KEEPALIVE_TIMEOUT=60

# Health Checker Connection Pool
HEALTH_CHECKER_MAX_CONNECTIONS=10
HEALTH_CHECKER_TIMEOUT=5

# Request Handling
REQUEST_TIMEOUT=30
MAX_REQUEST_SIZE=10485760

# Logging
DEBUG=false
LOG_TO_FILE=true
STREAM_STDOUT=true